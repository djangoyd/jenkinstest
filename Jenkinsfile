pipeline{
    agent any{
        stages{
            stage("Build"){
                steps{
                  sh  'echo "Here is build step"'
                }
            }
            stage("Test"){
                steps{
                   sh 'echo "Here is test step"'
                }
            }
            stage("Deployement"){
                steps{
                  sh  'echo "Here is Deployement step"'
                }
            }
        }
    }
}




